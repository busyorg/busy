    "classnames": "^2.2.5",
    "config": "^1.26.2",
    "css-loader": "^0.28.0",
    "debug": "~2.2.0",
    "del": "^3.0.0",
    "diff-match-patch": "^1.0.0",
    "ejs": "^2.5.6",
    "embedjs": "^0.0.4",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^2.1.2",
