{
  "name": "busy",
  "version": "2.0.0",
  "engines": {
    "node": ">=7.10.1",
    "npm": "=5.3.0"
  },
  "bin": "./server",
  "main": "./server",
  "private": true,
  "scripts": {
    "dev-server": "run-p dev-server:auth-server dev-server:client",
    "dev-server:client": "webpack-dev-server --config ./webpack/webpack-dev-server.js",
    "dev-server:auth-server": "node ./server/auth-server.js",
    "start": "cross-env NODE_ENV=production npm run run:server",
    "dev": "cross-env NODE_ENV=development run-p build:server:dev run:server",
    "test": "cross-env IMG_HOST=https://img.busy.org jest",
    "test:watch": "cross-env IMG_HOST=https://img.busy.org jest --watch",
    "test:coverage": "cross-env IMG_HOST=https://img.busy.org jest --coverage",
    "eslint-autofix": "eslint --fix \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "run:server": "nodemon --watch busy.server.js busy.server.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --progress --config ./webpack/webpack.server.config.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --config ./webpack/webpack.server.config.js",
    "build:client": "webpack -p --config ./webpack/webpack.prod.config.js",
    "build": "cross-env NODE_ENV=production npm run build:client && npm run build:server",
    "heroku-prebuild": "npm i -g pm2 && pm2 install pm2-logrotate",
    "heroku-postbuild": "npm run build"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "dependencies": {
    "antd": "^2.13.8",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.18.0",
    "bluebird": "^3.4.6",
    "body-parser": "~1.13.2",
    "bs58": "^4.0.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.17",
    "config": "^1.26.2",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "debug": "~2.2.0",
    "diff-match-patch": "^1.0.0",
    "embedjs": "^0.0.10",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "filesize": "^3.5.11",
    "history": "^4.6.3",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-style-loader": "^4.0.0",
    "js-cookie": "^2.1.4",
    "js-logger": "^1.3.0",
    "json-loader": "^0.5.7",
