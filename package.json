    "keycode": "^2.1.6",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lodash": "^4.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "matchmedia": "^0.1.2",
    "morgan": "^1.8.2",
    "newrelic": "^1.40.0",
